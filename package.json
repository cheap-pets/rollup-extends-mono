{
  "name": "@cheap-pets/rollup-extends-mono",
  "version": "1.0.0",
  "description": "Utilities, plugins, and preset configurations for Rollup",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build:custom": "pnpm --filter \"custom-example\" run build",
    "pub": "pnpm --filter=@cheap-pets/rollup-* publish --access=public"
  },
  "author": "scrollbar-ww",
  "license": "MIT",
  "private": true,
  "config": {
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "pnpm": {
    "packageExtensions": {
      "unplugin-vue2": {
        "peerDependencies": {
           "@vue/compiler-sfc": "^2.7.16"
        }
      }
    }
  }
}
